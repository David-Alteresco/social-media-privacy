}

.medium_bubble_text_judge {
    height: 230px;
}

.medium_bubble_text_user {
    height: 120px;
}

.speech-bubble-right:after {
    content: '';
    position: absolute;
    border: 20px solid transparent;
    border-top-color: forestgreen;
    left: 95%;
}

.speech-bubble-left:after {
    content: '';
    position: absolute;
    border: 20px solid transparent;
    border-top-color: seagreen;
    right: 90%;
}

